<plugin name='easyui' version='1.5' grailsVersion='2.0 &gt; *'>
  <author>Leandro Guindani Gehlen</author>
  <authorEmail>leandrogehlen@gmail.com</authorEmail>
  <title>JQuery EasyUI from Grails</title>
  <description>Supplies jQuery EasyUI resources and taglibs. Depends on jQuery EasyUI plugin. See http://www.jeasyui.com/</description>
  <documentation>http://grails.org/plugin/easyui</documentation>
  <type>EasyuiGrailsPlugin</type>
  <resources>
    <resource>EasyuiPluginResources</resource>
    <resource>org.grails.plugins.easyui.EasyuiTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>